  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx run-many --target=serve --projects=frontend,backend --parallel=2",
    "build": "nx build",
    "test": "nx test",
    "format": "nx format:write --files=apps/frontend/src,apps/frontend-e2e/src,apps/backend/src",
    "format:staged": "pretty-quick --staged",
    "lint": "nx affected:lint --files=apps/frontend/src,apps/frontend-e2e/src,apps/backend/src",
    "lint:all": "nx run-many --target=lint --projects=frontend,backend,frontend-e2e --parallel=3",
    "prepare": "husky install",
    "heroku-postbuild": "nx build $PROJECT_NAME --prod"
  },
  "private": true,
